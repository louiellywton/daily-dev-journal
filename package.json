{
  "name": "daily-dev-journal",
  "version": "1.0.0",
  "description": "A comprehensive daily development journal and productivity tracker for developers with intelligent analytics, automated insights, and multi-format data export capabilities",
  "main": "src/index.js",
  "bin": {
    "daily-journal": "./src/index.js",
    "ddj": "./src/index.js"
  },
  "files": [
    "src/",
    "scripts/",
    "config/",
    "README.md",
    "LICENSE",
    "CONTRIBUTING.md"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "node src/index.js interactive",
    "daily-update": "node scripts/daily-update.js",
    "generate-report": "node scripts/generate-report.js",
    "update-readme": "node scripts/update-readme-stats.js",
    "data-stats": "node scripts/data-management.js stats",
    "data-archive": "node scripts/data-management.js archive",
    "data-cleanup": "node scripts/data-management.js cleanup",
    "data-optimize": "node scripts/data-management.js optimize",
    "logs": "node scripts/data-management.js logs",
    "logs:analyze": "node scripts/data-management.js logs",
    "logs:optimize": "node scripts/data-management.js optimize-logs",
    "logs:compress": "node scripts/data-management.js compress-logs",
    "logs:stats": "node scripts/data-management.js logs-stats",
    "export": "node src/index.js export",
    "export:json": "node src/index.js export -f json",
    "export:csv": "node src/index.js export -f csv",
    "export:markdown": "node src/index.js export -f md",
    "stats": "node src/index.js stats",
    "stats:weekly": "node src/index.js stats -d 7",
    "stats:monthly": "node src/index.js stats -d 30",
    "stats:advanced": "node src/index.js advanced-stats",
    "stats:vector": "node src/index.js advanced-stats -d 30",
    "stats:hybrid": "node src/index.js hybrid-stats",
    "goals": "node src/index.js goals -l",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ scripts/",
    "lint:fix": "eslint src/ scripts/ --fix",
    "format": "prettier --write .",
    "validate": "npm run lint && npm run test",
    "setup": "npm install && node src/index.js --help"
  },
  "keywords": [
    "developer-tools",
    "productivity",
    "journal",
    "tracking",
    "automation",
    "daily-habits",
    "self-improvement",
    "analytics",
    "cli",
    "nodejs",
    "github-actions",
    "streak-tracking",
    "coding-habits",
    "development-tracking",
    "progress-tracker",
    "goal-management"
  ],
  "author": "louiellywton",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.6",
    "ml-matrix": "^6.12.1",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/louiellywton/daily-dev-journal.git"
  },
  "bugs": {
    "url": "https://github.com/louiellywton/daily-dev-journal/issues"
  },
  "homepage": "https://github.com/louiellywton/daily-dev-journal#readme"
}
